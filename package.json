{
  "name": "real-time-plotting-app",
  "version": "0.0.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build --base-href ./ && electron .",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "electron .",
    "electron-build": "ng build && electron .",
    "electron-windows-release": "npm run electron-build && electron-packager ./ RealTimePlottingApp --platform=win32 --overwrite",
    "electron-linux-release": "npm run electron-build && npx @electron/packager ./ RealTimePlottingAppForLinux --platform=linux --arch=arm64 --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/cdk": "^18.2.7",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/material": "^18.2.7",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "chart.js": "^4.4.4",
    "echarts": "^5.5.1",
    "express": "^4.21.1",
    "ng2-charts": "^6.0.1",
    "node-windows": "^1.0.0-beta.8",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uplot": "^1.6.31",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.3",
    "@angular/cli": "^18.1.3",
    "@angular/compiler-cli": "^18.1.0",
    "@types/jasmine": "~5.1.0",
    "cross-var": "^1.1.0",
    "electron": "^32.1.2",
    "electron-packager": "^17.1.2",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
  }
}